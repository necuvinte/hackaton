extends layout

block content
    if contacts
        each contact, i in contacts
            div
            ul.red-text
                if contact.JUDET
                    li.red-texte #{contact.JUDET}
                if contact.TELEFON
                    li #{contact.TELEFON}
                if contact.E-MAIL
                    li #{contact.E-MAIL}
                if contact.ADRESA
                    li #{contact.ADRESA}
                if contact.SERVICII
                    li #{contact.SERVICII}